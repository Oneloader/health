	height: 12px;
}

.bx-wrapper .bx-pager.bx-default-pager a {
	position: absolute;
	display: block;
	width: 12px;
	height: 12px;
	text-indent: -9999px;
	line-height: 1.0;
}

.pc .bx-wrapper .bx-pager.bx-default-pager a:hover,
.bx-wrapper .bx-pager.bx-default-pager a.active {
